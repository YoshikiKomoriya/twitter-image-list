{
  "name": "twitter-image-list",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && nuxt --env.NODE_TLS_REJECT_UNAUTHORIZED=0",
    "debug": "node --inspect node_modules/.bin/nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:css": "stylelint \"**/*.{css,scss,sass,js,ts,vue}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:css",
    "mock": "'./mock/node_modules/ts-node-dev/lib/bin.js' --project './mock/tsconfig.json' './mock/index.ts'",
    "jest": "jest --passWithNoTests",
    "test:ci": "yarn mock & sleep 5 && yarn jest",
    "test": "yarn mock & sleep 3 && yarn jest ; kill -9 `pgrep -f 'yarn|mock'`"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/pwa": "^3.3.5",
    "firebase": "^9.5.0",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.2",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "jest": "^26.6.3",
    "jest-express": "^1.12.0",
    "prettier": "^2.5.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^2.6.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "ts-jest": "^26.5.3",
    "vue-jest": "^3.0.4"
  }
