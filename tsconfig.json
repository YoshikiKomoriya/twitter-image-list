{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",

    "allowJs": true,
    "sourceMap": true,
    "removeComments": true,
    "experimentalDecorators": true,

    "strict": true,

    "moduleResolution": "Node",
    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true
  }
}
